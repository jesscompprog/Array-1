            System.out.println("Enter Value @ index" + i + ":");
             A[i] = input.nextInt();{
        
        }
        boolean isPrime = true;        
        {
                                                        
             for (int j = 2; j < i; j++) {
                if (i % j == 0) {
                    isPrime = false;
                    break;
                    
                }
                
             }
             if(isPrime){
                 System.out.println(i + " is a Prime");
                 
             }{
            int num1 = Integer.parseInt(JOptionPane.showInputDialog("Enter Number 1"));
